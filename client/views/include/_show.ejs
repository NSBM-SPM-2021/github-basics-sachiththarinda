<div class="form-title text-center">
    <h2 class="text-dark">Registered Patients</h2>
    
</div>

<% for(var i = 0; i < patients.length; i++) { %>
    
<tr>
    <td><%= i + 1 %></td>
    <td><%= patients[i].nic %></td>
    <td><%= patients[i].name %></td>
    <td><%= patients[i].age %></td>
    <td><%= patients[i].vaccine %></td>
    <td>
        <a href="/update-patient?id=<%= patients[i]._id %>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a href="#" class="btn border-shadow delete">
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>

<% } %>